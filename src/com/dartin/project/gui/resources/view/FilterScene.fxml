<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<TabPane stylesheets="@../Styles/Pane.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <tabs>
      <Tab closable="false" text="Name">
         <content>
            <VBox prefHeight="0.0" prefWidth="107.0" styleClass="Box" stylesheets="@../Styles/Pane.css">
               <children>
                  <TextField stylesheets="@../Styles/TextField.css">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </TextField>
                  <HBox alignment="CENTER_RIGHT">
                     <children>
                        <Button id="smallBut" mnemonicParsing="false" stylesheets="@../Styles/Buttons.css" text="Search" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </content>
      </Tab>
      <Tab closable="false" text="Size">
         <content>
            <VBox prefHeight="0.0" prefWidth="107.0" styleClass="Box" stylesheets="@../Styles/Pane.css">
               <children>
                  <Slider blockIncrement="1.0" majorTickUnit="1.0" max="4.0" min="1.0" minorTickCount="1" showTickLabels="true" stylesheets="@../Styles/Slider.css" value="1.0">
                     <VBox.margin>
                        <Insets bottom="10.0" top="5.0" />
                     </VBox.margin>
                  </Slider>
                  <HBox alignment="CENTER_RIGHT">
                     <children>
                        <Button id="smallBut" mnemonicParsing="false" stylesheets="@../Styles/Buttons.css" text="Search" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </content>
      </Tab>
      <Tab closable="false" text="Usage">
         <content>
            <VBox prefHeight="0.0" prefWidth="107.0" styleClass="Box" stylesheets="@../Styles/Pane.css">
               <children>
                  <ChoiceBox prefHeight="43.0" prefWidth="193.0" stylesheets="@../Styles/ChoiseBox.css">
                     <VBox.margin>
                        <Insets bottom="10.0" left="5.0" top="5.0" />
                     </VBox.margin>
                  </ChoiceBox>
                  <HBox alignment="CENTER_RIGHT">
                     <children>
                        <Button id="smallBut" mnemonicParsing="false" stylesheets="@../Styles/Buttons.css" text="Search" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </content>
      </Tab>
      <Tab closable="false" text="Date">
         <content>
            <VBox prefHeight="97.0" prefWidth="248.0" styleClass="Box" stylesheets="@../Styles/Pane.css">
               <children>
                  <DatePicker styleClass="choice-box" stylesheets="@../Styles/ChoiseBox.css">
                     <VBox.margin>
                        <Insets bottom="10.0" left="5.0" top="5.0" />
                     </VBox.margin>
                  </DatePicker>
                  <HBox alignment="CENTER_RIGHT">
                     <children>
                        <Button id="smallBut" mnemonicParsing="false" stylesheets="@../Styles/Buttons.css" text="Search" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </content>
      </Tab>
   </tabs>
</TabPane>
