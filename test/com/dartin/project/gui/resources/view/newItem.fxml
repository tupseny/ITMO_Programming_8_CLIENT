<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="409.0"
            prefWidth="360.0"
            xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.dartin.project.gui.controller.NewItemController">
	<Label layoutX="10.0"
	       layoutY="27.0"
	       text="Name"/>
	<Label layoutX="14.0"
	       layoutY="66.0"
	       text="Usage"/>
	<Label layoutX="12.0"
	       layoutY="227.0"
	       text="Size"/>
	<Label layoutX="10.0"
	       layoutY="184.0"
	       text="Date"/>
	<TextField fx:id="textFieldName"
	           layoutX="76.0"
	           layoutY="23.0"/>
	<Slider fx:id="sliderSize"
	        blockIncrement="1.0"
	        layoutX="33.0"
	        layoutY="279.0"
	        majorTickUnit="1.0"
	        max="4.0"
	        min="1.0"
	        minorTickCount="0"
	        prefHeight="38.0"
	        prefWidth="273.0"
	        showTickLabels="true"
	        showTickMarks="true"
	        snapToTicks="true"
	        value="1.0"
	        AnchorPane.bottomAnchor="120.0"
	        AnchorPane.leftAnchor="30.0"
	        AnchorPane.rightAnchor="30.0"/>
	<DatePicker fx:id="datePicker"
	            layoutX="54.0"
	            layoutY="180.0"/>
	<ButtonBar prefHeight="40.0"
	           prefWidth="200.0"
	           AnchorPane.bottomAnchor="10.0"
	           AnchorPane.rightAnchor="10.0">
		<buttons>
			<Button mnemonicParsing="false"
			        onAction="#handleButtonOk"
			        text="Ok"/>
			<Button mnemonicParsing="false"
			        onAction="#handleButtonCancel"
			        text="Cancel"/>
		</buttons>
	</ButtonBar>
	<ChoiceBox fx:id="choiceBoxUsage"
	           layoutX="75.0"
	           layoutY="62.0"
	           prefWidth="150.0"/>
</AnchorPane>
